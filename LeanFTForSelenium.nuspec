<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <!-- The identifier that must be unique within the hosting gallery -->
    <id>LeanFT.Selenium</id>

    <!-- A human-friendly title of the package -->
    <title>LeanFT for Selenium DotNet (C#) SDK</title>

    <!-- The package version number that is used when resolving dependencies -->
    <version>$version$</version>

    <!-- Authors contain text that appears directly on the gallery -->
    <authors>$author$</authors>

    <!-- Owners are typically nuget.org identities that allow gallery
            users to easily find other packages by the same owners.  -->
    <owners>$author$</owners>

    <!-- License and project URLs provide links for the gallery -->
    <licenseUrl>http://www.apache.org/licenses/LICENSE-2.0</licenseUrl>
    <projectUrl>https://github.com/MicroFocus/leanft-selenium-dotnet-sdk</projectUrl>

    <!-- The icon is used in Visual Studio's package manager UI -->
    <iconUrl>https://github.com/MicroFocus/leanft-selenium-dotnet-sdk/blob/master/Resources/LeanFT_64x64.png?raw=true</iconUrl>

    <!-- If true, this value prompts the user to accept the license when
            installing the package. -->
    <requireLicenseAcceptance>false</requireLicenseAcceptance>

    <!-- Any details about this particular release -->
    <releaseNotes>Initial version of LeanFT for Selenium DotNet (C#) SDK.</releaseNotes>

    <!-- The description can be used in package manager UI. Note that the
            nuget.org gallery uses information you add in the portal. -->
    <description>LeanFT for Selenium DotNet (C#) SDK that extends the WebDriver API with additional locators and utilities.</description>
    
    <!-- The summary of the package -->
    <summary>LeanFT for Selenium DotNet (C#) SDK that extends the WebDriver API with additional locators and utilities.</summary>

    <!-- Copyright information -->
    <copyright>$copyright$</copyright>

    <!-- Tags appear in the gallery and can be used for tag searches -->
    <tags>LeanFT Selenium Test Functional Automation Browser</tags>

    <!-- Dependencies are automatically installed when the package is installed -->
    <dependencies>
      <dependency id="Selenium.WebDriver" version="2.53.1" />
      <dependency id="Selenium.Support" version="2.53.1" />
    </dependencies>
  </metadata>

  <!-- A readme.txt to display when the package is installed -->
  <files>
    <file src="readme.txt" target="" />
  </files>
</package>